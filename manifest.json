{
  "name": "meejegak",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage"],
  "content_scripts": [
      {
          "matches": ["*://meet.google.com/*"],
          "js": [
              "content.js"
          ],
          "css": []
      }
  ],
  "browser_action": {
      "default_icon": {
          "128": "images/icon128.png"
      }
  },
  "icons": {
      "128": "images/icon128.png"
  }
}
